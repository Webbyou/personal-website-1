        "gatsby-transformer-remark": "2.3.12",
        "gatsby-transformer-sharp": "2.1.19",
        "husky": "1.3.1",
        "lint-staged": "8.1.3",
        "prettier": "1.16.4",
        "typescript": "3.4.5"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsc --noEmit && lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx,jsx}": [
            "eslint . --fix",
            "git add"
        ]
    },
    "scripts": {
        "start": "yarn dev",
        "dev": "rm -rf .cache && gatsby develop",
        "build": "rm -rf .cache && gatsby build",
        "serve": "gatsby serve",
        "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix && tsc --noEmit"
    }
}
